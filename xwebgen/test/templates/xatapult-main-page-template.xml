  <body>
    <a name="top" class="top">&#160;</a>
    <div id="container">
      <div id="header">
        <img src="images/spacer.gif" alt="Xatapult home" width="349" height="102" border="0" usemap="#LinkHomepage" />
        <map name="LinkHomepage" id="LinkHomepage"><area shape="rect" coords="18,48,345,83" href="http://www.xatapult.nl/home" alt="Xatapult home"
           /></map>
      </div>
      <div id="mainContent">
        
        <div class="contentwrapper">
          <!-- MAIN CONTENTS: -->
          <page-contents-expand dref-section-document="../data/sections-home.xml" xmlns="http://www.xtpxlib.nl/ns/xwebgen"/>
        </div>

        <!-- RIGHT INFO COLUMN: -->
        <section-expand dref-section-document="../data/main-ui-settings.xml" section-idref="info-column" xmlns="http://www.xtpxlib.nl/ns/xwebgen"/>
        
      </div>
      <div id="copyright">
        <a href="http://www.strikdesign.nl" target="_blank">Webdesign (c) 2009 Strik Design</a>
      </div>
      <div id="naw">
        <table width="100%" border="0" cellpadding="0" cellspacing="0">
          <tr>
            <td align="right" style="font-size: 8pt">&#160;e-mail:&#160;</td>
            <td align="left" style="font-size: 8pt"><a href="mailto:${email}">${email}</a></td>
          </tr>
          <tr>
            <td align="right" style="font-size: 8pt">&#160;tel:&#160;</td>
            <td align="left" class="tel" style="font-size: 8pt">${phonenumber}</td>
          </tr>
        </table>
      </div>
      <section-expand dref-section-document="../data/main-ui-settings.xml" section-idref="main-menu" xmlns="http://www.xtpxlib.nl/ns/xwebgen"/>
